{
  "name": "ops-api",
  "version": "0.0.0",
  "main": "dist/server.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinystacks/ops-api.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./dist; rm -rf ./store;",
    "hard-clean": "rm -rf ./dist; rm -rf node_modules; rm package-lock.json",
    "clean-build": "npm run clean; npm ci; npm run build",
    "prepack": "npm run clean-build",
    "dev": "npm run clean; bash ./dev.sh",
    "lint": "./node_modules/.bin/eslint ./src",
    "lint-fix": "./node_modules/.bin/eslint --fix ./src",
    "lint-tests": "./node_modules/.bin/eslint --config .eslintrc.test.json ./test",
    "lint-fix-tests": "./node_modules/.bin/eslint --config .eslintrc.test.json --fix ./test",
    "local": "bash ./local.sh",
    "local-s3": "npm run clean-build; AWS_PROFILE=ts AWS_REGION=us-west-2 CONFIG_PATH=s3://ops-stacks-dev-config-storage-bucket-us-west-2/group:tinystacks-beta/layout-sample.yml NODE_ENV=dev node ./dist/server.js",
    "qa": "npm run lint && npm run test-cov",
    "setup-test-lint": "node ./scripts/setup-test-lint.cjs",
    "staging": "bash ./staging.sh",
    "start": "node ./dist/server.js",
    "test": "jest",
    "test-cov": "jest --coverage",
    "test-file": "jest ./test/utils/parsing-utils.test.ts",
    "view-test-cov": "jest --coverage || true && open coverage/lcov-report/index.html"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudwatch": "^3.264.0",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/preset-env": "^7.20.2",
    "@types/cached": "^6.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/http-errors": "^2.0.1",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^29.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/lodash.difference": "^4.5.7",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.isnil": "^4.0.7",
    "@types/lodash.isobject": "^3.0.7",
    "@types/lodash.upperfirst": "^4.3.7",
    "@types/node": "^18.11.18",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "babel-jest": "^29.5.0",
    "dayjs": "^1.11.7",
    "depcheck": "^1.4.3",
    "eslint": "^8.32.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.4.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isempty": "^4.4.0",
    "ts-jest": "^29.0.5",
    "ts-jest-resolver": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@aws-sdk/client-api-gateway": "^3.267.0",
    "@aws-sdk/client-s3": "^3.288.0",
    "@aws-sdk/credential-providers": "^3.282.0",
    "@tinystacks/ops-core": "file:../ops-core/tinystacks-ops-core-0.3.2.tgz",
    "@tinystacks/ops-model": "file:../ops-model/tinystacks-ops-model-0.4.1.tgz",
    "@types/react": "^18.0.28",
    "body-parser": "^1.20.1",
    "cached": "^6.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-openapi": "^12.1.0",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "js-yaml": "^4.1.0",
    "json-refs": "^3.0.15",
    "lodash.camelcase": "^4.3.0",
    "lodash.difference": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.isnil": "^4.0.0",
    "lodash.isobject": "^3.0.2",
    "lodash.upperfirst": "^4.3.1",
    "react": "^18.2.0",
    "swagger-ui-express": "^4.6.0",
    "yamljs": "^0.3.0"
  }
}
